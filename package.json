{
  "name": "@mapbox/togeojson",
  "version": "0.16.0",
  "description": "convert KML and GPX to GeoJSON",
  "main": "togeojson.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "eslint togeojson.js && tape test/*.js"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "glob": "^7.0.5",
    "tape": "~4.6.0"
  },
  "bin": {
    "togeojson": "togeojson"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/togeojson.git"
  },
  "browser": {
    "xmldom": false
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "@xmldom/xmldom": "0.8.1",
    "concat-stream": "2.0.0",
    "minimist": "1.2.5"
  },
  "keywords": [
    "kml",
    "geojson",
    "gpx",
    "geo",
    "parser",
    "formatter",
    "input",
    "leaflet",
    "maps"
  ]
}
